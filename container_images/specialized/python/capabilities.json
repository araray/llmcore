{
  "name": "llmcore-sandbox-python",
  "version": "1.0.0",
  "tier": "specialized",
  "description": "Python development environment for LLMCore agent sandboxes",
  "capabilities": ["python", "shell", "git"],
  "tools": ["python3", "pip", "venv", "git", "bash", "curl", "grep", "sed", "awk"],
  "parent_image": "llmcore-sandbox-base:1.0.0",
  "default_access_mode": "restricted",
  "resource_limits": {
    "memory_mb": 1024,
    "cpu_cores": 2,
    "timeout_seconds": 600,
    "max_processes": 100
  },
  "environment": {
    "LLMCORE_SANDBOX": "1",
    "LLMCORE_IMAGE_TIER": "specialized",
    "PYTHONUNBUFFERED": "1"
  }
}
